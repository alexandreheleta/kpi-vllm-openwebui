{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "panels": [],
      "title": "Key Metrics",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Users active in the last 30 days",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": { "mode": "absolute", "steps": [{ "color": "#5794F2", "value": null }] },
          "unit": "short",
          "decimals": 0
        }
      },
      "gridPos": { "h": 5, "w": 8, "x": 0, "y": 1 },
      "id": 1,
      "options": { "colorMode": "value", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "targets": [{ "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "openwebui_users_active_30d", "refId": "A" }],
      "title": "Active Users",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Average end-to-end response time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 5 }, { "color": "red", "value": 10 }] },
          "unit": "s",
          "decimals": 2
        }
      },
      "gridPos": { "h": 5, "w": 8, "x": 8, "y": 1 },
      "id": 2,
      "options": { "colorMode": "value", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "targets": [{ "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "avg(rate(vllm:e2e_request_latency_seconds_sum[$__rate_interval]) / rate(vllm:e2e_request_latency_seconds_count[$__rate_interval]))", "refId": "A" }],
      "title": "Avg Response Time",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Tokens generated by AI models in selected time range",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": { "mode": "absolute", "steps": [{ "color": "#73BF69", "value": null }] },
          "unit": "short",
          "decimals": 0
        }
      },
      "gridPos": { "h": 5, "w": 8, "x": 16, "y": 1 },
      "id": 3,
      "options": { "colorMode": "value", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "targets": [{ "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "sum(increase(vllm:generation_tokens_total[$__range]))", "refId": "A" }],
      "title": "Tokens Generated",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 6 },
      "id": 101,
      "panels": [],
      "title": "Trends",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "axisBorderShow": false, "axisPlacement": "auto", "drawStyle": "line", "fillOpacity": 30, "lineInterpolation": "smooth", "lineWidth": 2, "showPoints": "never", "stacking": { "mode": "none" } },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 7 },
      "id": 5,
      "options": { "legend": { "calcs": ["lastNotNull", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true }, "tooltip": { "mode": "multi", "sort": "desc" } },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "openwebui_messages_total", "legendFormat": "Total AI Responses", "refId": "A" }
      ],
      "title": "Cumulative AI Responses",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "hideFrom": { "legend": false, "tooltip": false, "viz": false } }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 7 },
      "id": 6,
      "options": { "legend": { "displayMode": "table", "placement": "right", "showLegend": true, "values": ["value", "percent"] }, "pieType": "donut", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "tooltip": { "mode": "single" } },
      "targets": [{ "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "openwebui_model_usage", "legendFormat": "{{model}}", "refId": "A" }],
      "title": "Model Usage Distribution",
      "type": "piechart"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 15 },
      "id": 102,
      "panels": [],
      "title": "User Leaderboard",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": { "align": "auto", "cellOptions": { "type": "auto" }, "inspect": false },
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }
        },
        "overrides": [{ "matcher": { "id": "byName", "options": "AI Responses" }, "properties": [{ "id": "custom.cellOptions", "value": { "mode": "gradient", "type": "gauge" } }] }]
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 },
      "id": 7,
      "options": { "cellHeight": "sm", "footer": { "show": false }, "showHeader": true, "sortBy": [{ "desc": true, "displayName": "AI Responses" }] },
      "targets": [{ "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "openwebui_user_messages", "format": "table", "instant": true, "refId": "A" }],
      "title": "All Users - AI Responses",
      "transformations": [{ "id": "organize", "options": { "excludeByName": { "Time": true, "job": true, "service_name": true, "__name__": true }, "renameByName": { "Value": "AI Responses", "user_name": "User" } } }],
      "type": "table"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "tags": ["open-webui", "executive", "kpi"],
  "templating": { "list": [] },
  "time": { "from": "now-7d", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "OpenWebUI - Executive KPI",
  "uid": "openwebui-executive",
  "version": 1
}
