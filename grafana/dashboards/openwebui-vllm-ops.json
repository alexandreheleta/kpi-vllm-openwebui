{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "panels": [],
      "title": "Infrastructure Health",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "KV cache utilization per model",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 50 }, { "color": "red", "value": 80 }] },
          "decimals": 0,
          "max": 100,
          "min": 0,
          "unit": "percent"
        },
        "overrides": [
          { "matcher": { "id": "byRegexp", "options": ".*Coder.*" }, "properties": [{ "id": "displayName", "value": "Coder" }] },
          { "matcher": { "id": "byRegexp", "options": ".*0.5B.*" }, "properties": [{ "id": "displayName", "value": "Chat" }] }
        ]
      },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 1 },
      "id": 1,
      "options": { "displayMode": "basic", "minVizHeight": 10, "minVizWidth": 0, "orientation": "horizontal", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "showUnfilled": true, "valueMode": "color" },
      "targets": [{ "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "vllm:kv_cache_usage_perc{model_name=~\"$model_name\"} * 100", "legendFormat": "{{model_name}}", "refId": "A" }],
      "title": "KV Cache",
      "type": "bargauge"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Requests currently running and waiting in queue",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "axisBorderShow": false, "axisPlacement": "auto", "drawStyle": "bars", "fillOpacity": 80, "lineWidth": 1, "showPoints": "never", "stacking": { "mode": "normal" } },
          "unit": "short"
        }
      },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 1 },
      "id": 2,
      "options": { "legend": { "calcs": [], "displayMode": "list", "placement": "bottom", "showLegend": true }, "tooltip": { "mode": "multi", "sort": "desc" } },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "vllm:num_requests_running{model_name=~\"$model_name\"}", "legendFormat": "Running", "refId": "A" },
        { "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "vllm:num_requests_waiting{model_name=~\"$model_name\"}", "legendFormat": "Waiting", "refId": "B" }
      ],
      "title": "Request Queue",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 7 },
      "id": 101,
      "panels": [],
      "title": "Performance",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Time to first token latency percentiles",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "axisCenteredZero": false, "axisColorMode": "text", "axisPlacement": "auto", "drawStyle": "line", "fillOpacity": 10, "lineInterpolation": "smooth", "lineWidth": 2, "pointSize": 5, "showPoints": "never", "spanNulls": false, "stacking": { "mode": "none" }, "thresholdsStyle": { "mode": "off" } },
          "unit": "s",
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "P99" }, "properties": [{ "id": "color", "value": { "fixedColor": "#5794F2", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "P95" }, "properties": [{ "id": "color", "value": { "fixedColor": "#73BF69", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "P50" }, "properties": [{ "id": "color", "value": { "fixedColor": "#FF9830", "mode": "fixed" } }] }
        ]
